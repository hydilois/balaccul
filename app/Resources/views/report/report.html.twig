{% extends 'base.html.twig' %}
{% block title %}{{parent()}} Report{% endblock %}
{% block contenu %}
 <ol class="breadcrumb">
    <li>
      <a href="#">BALACCUL</a>
    </li>
    <li>
      <a href="#">Reports</a>
    </li>
    <li class="active">
      Trial Balance - Member Share
    </li>
  </ol>
    <div class="row">
        <fieldset>
            <legend>
                TRIAL BALANCE
            </legend>
        <div class=" col-lg-offset-2 col-lg-8 col-md-8 col-sm-8 col-xs-8 ">
            <div class="portlet">
                <div class="portlet-heading bg-custom">
                    <div class="portlet-widgets pull-left">
                        <span class="divider"></span>
                       {#  <a data-toggle="collapse" data-parent="#accordion1" href="#bg-info1" class="collapsed"><i class="ion-minus-round"></i></a> #}
                        <span class="divider"></span>
                    </div>
                        <h3 class="portlet-title">
                            DATE RANGE
                        </h3>
                    <div class="clearfix"></div>
                </div>
                <div id="bg-info1" class="panel-collapse collapse" style="display:block">
                    <div class="portlet-body">
                        <div class="p-20">
                            <form target="_blank" method="POST" action="{{ path('trialbalance_report') }}" class="form-horizontal">
                                <div class="form-group">
                                    <label class="control-label col-sm-4"> From </label>
                                    <div class="col-sm-6">
                                        <div class="input-daterange input-group" id="date-range">
                                            <input type="text" class="form-control" name="start" />
                                            <span class="input-group-addon bg-custom b-0 text-white">To</span>
                                            <input type="text" class="form-control" name="end" />
                                        </div>
                                        <br>
                                        <input  type="submit" class="btn btn-default" value="Valider"/>
                                    </div>
                                </div>
                                
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        </fieldset>
        </div> <!-- End row -->

         {# Hidden inputs #}
    <select name="accountNumber" class="selecteur-zone form-control hide" id=""></select>



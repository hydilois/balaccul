{% extends 'base.html.twig' %}
{% block title %}{{parent()}} Ledger{% endblock %}
{% block contenu %}
 <ol class="breadcrumb">
    <li>
      <a href="#">HYMIF</a>
    </li>
    <li>
      <a href="#">Reports</a>
    </li>
    <li class="active">
      Trial Balance
    </li>
  </ol>
    <div style="margin-top:45px" class="row">
        <fieldset>
            <legend>
                TRIAL BALANCE
            </legend>
        <div class=" col-lg-offset-2 col-lg-8 col-md-8 col-sm-8 col-xs-8 ">
            <div class="portlet">
                <div class="portlet-heading bg-custom">
                    <div class="portlet-widgets pull-left">
                        <span class="divider"></span>
                        <span class="divider"></span>
                    </div>
                        <h3 class="portlet-title">
                            DATE RANGE
                        </h3>
                    <div class="clearfix"></div>
                </div>
                <div id="bg-info1" class="panel-collapse collapse" style="display:block">
                    <div class="portlet-body">
                        <div class="p-20">
                            <form target="_blank" method="POST" action="{{ path('trial_balance_report') }}" class="form-horizontal">
                                <div class="form-group">
                                    <label class="control-label col-sm-4"> From </label>
                                    <div class="col-sm-6">
                                        <div class="input-daterange input-group" id="date-range">
                                            <input type="text" class="form-control" name="start" />
                                            <span class="input-group-addon bg-custom b-0 text-white">To</span>
                                            <input type="text" class="form-control" name="end" />
                                        </div>
                                        <br>
                                        <input  type="submit" class="btn btn-default" value="Validate"/>
                                    </div>
                                </div>
                                
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        </fieldset>
        </div> <!-- End row -->

    <div style="margin-top:45px" class="row">
        <fieldset>
            <legend>
                FINANCIAL AFFAIRS
            </legend>
            <div class=" col-lg-offset-2 col-lg-8 col-md-8 col-sm-8 col-xs-8 ">
                <div class="portlet">
                    <div class="portlet-heading bg-primary">
                        <div class="portlet-widgets pull-left">
                            <span class="divider"></span>
                            <span class="divider"></span>
                        </div>
                        <h3 class="portlet-title">
                            DATE RANGE
                        </h3>
                        <div class="clearfix"></div>
                    </div>
                    <div id="bg-info1" class="panel-collapse collapse" style="display:block">
                        <div class="portlet-body">
                            <div class="p-20">
                                <form target="_blank" method="POST" action="{{ path('balance_sheet') }}" class="form-horizontal">
                                    <div class="form-group">
                                        <label class="control-label col-sm-4"> From </label>
                                        <div class="col-sm-6">
                                            <div class="input-daterange input-group" id="date-range1">
                                                <input type="text" class="form-control" name="start" />
                                                <span class="input-group-addon bg-primary b-0 text-white">To</span>
                                                <input type="text" class="form-control" name="end" />
                                            </div>
                                            <br>
                                            <input  type="submit" class="btn btn-primary" value="Validate"/>
                                        </div>
                                    </div>

                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </fieldset>
    </div>

         {# Hidden inputs #}
    <select name="accountNumber" class="selecteur-zone form-control hide" id=""></select>


    {% include '/report/extras_report.css.html.twig' %}
    {% include '/report/extras_report.js.html.twig' %}
{% endblock %}
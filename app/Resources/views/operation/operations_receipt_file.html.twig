<page backtop="10mm" backleft="10mm" backright="10mm" backbottom="10mm" footer="page;">
    <page_footer>
        <hr />
        <span>{{agency.abreviation}}</span><span style="margin-left:550px;">GreenSoft-Team</span>
    </page_footer>
    <table class="head">
        <tr>
            <td class="60p">
                 <b>{{agency.abreviation}}</b><br/>
                 <b>{{agency.name}}</b><br>
                 <b>Tel: {{agency.phoneNumber}}</b>
            </td>
            <td  style="text-align: right;" class="40p">
                <img style="height:60px;" src="./assets/images/users/logo_cu1.jpg"  alt="logo">
            </td>
        </tr>
    </table>

    <table class="title">
        <tr>
            <td class="25p" style="text-align: left;"></td>
            <td class="50p" style="text-align: center; font-size:20px"><strong>CASH IN RECEIPT</strong></td>
            <td class="25p" style="text-align: right;"></td>
        </tr>
    </table><br>
    <table>
        <tr>
            <td class="75p" style="text-align: right;">Date: <b>{{currentDate|date}}</b> </td>
            <td class="25p" style="text-align: right;">
            </td>
        </tr>
    </table>
   <table class="zoneoperation">
        <tr>
            <td  class="75p"> 
                    Account Number: <b> 
                            {{member.memberNumber}}
                            </b>
            </td>
            <td style="text-align: center; border-bottom: 1px solid black">Amount</td>
        </tr>
        <tr>
            <td colspan="1" class="75p" style="text-align: left; border-right: 1px solid black"> 
                    Account Name: <b> 
                    {% if testMember %}
                        {{member.name}}
                    {% else %}
                        {{member.socialReason}}
                    {% endif %}
                </b>
            </td>
            <td style="border:1px solid black; text-align:center; font-size:20px" class="25p"><b>{{totalTransaction}}</b></td>
        </tr>
        {% if representative %}
            <tr>
                <td colspan="1" class="75p" style="text-align: left;"> Representative: 
                    <b>
                        {{representative|upper}}
                    </b>
                </td>
                <td class="25"></td>
            </tr>
        {% endif %}
    </table>
    <table>
        <tr>
            <td class="50p">
                <h5 style="text-align: center">Purposes</h5>
                    <table class="zoneOperation">
                            {% set totalAcccountOperation = 0 %}
                        {% if accountOperations %}
                            {% for operation in accountOperations %}
                                <tr>
                                    <td class="50p">
                                        {% if operation.savingAccount %}
                                             Savings
                                        {% elseif operation.shareAccount %}
                                             Shares
                                        {% else %}
                                             Deposits
                                        {% endif %}
                                    </td>
                                    <td class="50p">{{operation.amount}}</td>
                                </tr>
                                {% set totalAcccountOperation = totalAcccountOperation + operation.amount %}
                            {% endfor %}

                        {% endif %}
                        {% if loanhistory.currentUser %}
                            {% if loanhistory.monthlyPayement %}
                            <tr>
                                <td class="50p">
                                    Main Loan Repayment
                                </td>
                                <td class="50p">{{loanhistory.monthlyPayement}}</td>
                            </tr>
                                {% set totalAcccountOperation = totalAcccountOperation + loanhistory.monthlyPayement %}
                            {% endif %}
                            {% if loanhistory.interest %}
                                <tr>
                                    <td class="50p">
                                        Loan Interest
                                    </td>
                                    <td class="50p">{{loanhistory.interest}}</td>
                                </tr>
                                    {% set totalAcccountOperation = totalAcccountOperation + loanhistory.interest %}
                            {% endif %}
                        {% endif %}
                        {% if others %}
                            {% for other in others %}
                                <tr>
                                    <td class="50p">
                                        {{other.name}}
                                    </td>
                                    <td class="50p">{{other.value}}</td>
                                </tr>
                                {% set totalAcccountOperation = totalAcccountOperation + other.value %}
                            {% endfor %}
                        {% endif %}
                            <tr>
                                <td class="50p" style="border-bottom:1px solid black; height: 2px"></td>
                                <td class="50p" style="border-bottom:1px solid black; height: 2px"></td>
                            </tr>
                            <tr>
                                <td class="50p"><b>TOTAL</b></td>
                                <td class="50p">{{totalAcccountOperation}}</td>
                            </tr>
                    </table>
                {# </div> #}
            </td>
            <td class="50p">
                <h5 style="text-align: center">Analytics</h5>
                    <table class="zoneOperation">
                        {% set totalMoney = 0 %}
                        {% for money in analytics %}
                            {% if money.value != 0 %}
                            <tr>
                                <td style="text-align:right" class="50p">{{money.name}}    x   {{money.value}} =  </td>
                                {% set total = 0 %}
                                {% set total = money.value * money.name %}
                                <td style="text-align:right" class="50p">{{total}}</td>
                            </tr>
                            {% set totalMoney = totalMoney + total %}
                            {% endif %}
                        {% endfor %}
                        <tr>
                            <td class="50p" style="border-bottom:1px solid black; height: 2px"></td>
                            <td class="50p" style="border-bottom:1px solid black; height: 2px"></td>
                        </tr>
                        <tr>
                            <td style="text-align:right;  height: 25px" class="50p"><b>TOTAL</b></td>
                            <td style="text-align:right; height: 25px" class="50p">{{totalMoney}}</td>
                        </tr>
                    </table>
            </td>
        </tr>
    </table>
    <table style="margin-bottom:0px">
        <tr>
            <td class="25p" style="border-bottom:1px dashed black;border-right:1px dashed black; text-align:center">MEMBER</td>
            <td class="25p" style="border-bottom:1px dashed black; text-align:center;">CASHER</td>
        </tr>
        <tr>
            <td class="15p" style="border-right:1px dashed black">
                <br><br>
            </td>
            <td class="15p">
                <br><br>
            </td>
        </tr>
    </table>


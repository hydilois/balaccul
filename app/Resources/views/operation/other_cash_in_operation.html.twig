                <div class="form-group">
                            <label class="control-label col-sm-4">Choose the operation</label>
                        <select name="accountNumber" class="form-control select2" required="required">
                        <option value="">&lt;------ Select ------&gt;</option>
                        {% for account in internalAccounts %}
                            <option value="{{account.id}}">{{account.accountName}} </option>
                        {% endfor %}
                    </select>
                </div>
                <div class="form-group">
                            <label class="control-label col-sm-4">Choose the member</label>
                        <select name="memberNumber" class="form-control select2">
                        <option value="">&lt;------ Select ------&gt;</option>
                        {% for member in members %}
                            <option value="{{member.id}}">{{member.name}} --{{member.memberNumber}} </option>
                        {% endfor %}
                    </select>
                </div>
                <div class="form-group">
                    <label for="">Representative</label>
                    <input name="representative" required="required" type="text" class="form-control" />
                </div>
                <div class="form-group">
                    <label for="">Amount</label>
                    <input name="amount" type="number" min="0" class="form-control" value="0" />
                </div>
                    <div class="form-group">
                        <input class="btn btn-default" type="submit" value="Save">

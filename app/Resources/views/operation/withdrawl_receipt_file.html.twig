<page backtop="10mm" backleft="10mm" backright="10mm" backbottom="10mm" footer="page;">
    <page_footer>
        <hr />
        <span>BALACCUL</span><span style="margin-left:550px;">GreenSoft-Team</span>
    </page_footer>


    <table>
        <tr>
            <td class="60p">
                 <b>{{agency.abreviation}}</b><br/>
                 <b>{{agency.name}}</b><br>
                 <b>{{agency.phoneNumber}}</b> <br>

            </td>
            <td  style="text-align: right;" class="40p">
                <img style="height:80px;" src="./assets/images/users/logo_cu1.jpg"  alt="logo">
            </td>
        </tr>
    </table>

    <table style="margin-top: 10px;">
        <tr>
            <td class="25p" style="text-align: left;"></td>
            <td class="50p" style="text-align: center; font-size:20px"><strong>
            {% if operation.typeOperation == "CREDIT"%}
                    DEPOSIT RECEIPT
            {% elseif  operation.typeOperation =="DEBIT" %}
                    WITHDRAWAL RECEIPT
            {% else %}
                RECEIPT
            {% endif %}
            </strong></td>
            <td class="25p" style="text-align: right;"></td>
        </tr>
    </table><br>


    <table>
        <tr>
            <td class="75p" style="text-align: right;">Date: <b>{{operation.dateOperation|date}}</b> </td>
            <td class="25p" style="text-align: right;">
            </td>
        </tr>
    </table> <br>

    <table class="zoneoperation">
        <tr>
            <td class="50p" style="text-align: left;">Account: <b>
                    {% if operation.savingAccount %}
                        {{operation.savingAccount}}

                    {% elseif operation.shareAccount %}
                        {{operation.shareAccount}}
                    {% else %}
                        {{operation.depositAccount}}
                    {% endif %}    
                </b>
        </td>
            <td class="50p" style="text-align: left;">Name: <b> 
                    {% if operation.savingAccount %}
                        {% if operation.savingAccount.moralMember %}
                            {{operation.savingAccount.moralMember|upper}}
                        {% else %}
                            {{operation.savingAccount.physicalMember|upper}}
                        {% endif %}

                    {% elseif operation.shareAccount %}
                        {% if operation.shareAccount.moralMember %}
                            {{operation.shareAccount.moralMember|upper}}
                        {% else %}
                            {{operation.shareAccount.physicalMember|upper}}
                        {% endif %}
                    {% else %}
                        {% if operation.depositAccount.moralMember %}
                            {{operation.depositAccount.moralMember|upper}}
                        {% else %}
                            {{operation.depositAccount.physicalMember |upper}}
                        {% endif %}
                    {% endif %}

                </b>
            </td>
        </tr>
        <tr>
            <td class="50p">Caisher: <b>{{operation.currentUser}}</b></td>
            <td class="50p">Phone: <b>
                                {% if operation.savingAccount %}
                                    {% if operation.savingAccount.moralMember %}
                                        {{operation.savingAccount.moralMember.phoneNumber}}
                                    {% else %}
                                        {{operation.savingAccount.physicalMember.phoneNumber}}
                                    {% endif %}

                                {% elseif operation.shareAccount %}
                                    {% if operation.shareAccount.moralMember %}
                                        {{operation.shareAccount.moralMember.phoneNumber}}
                                    {% else %}
                                        {{operation.shareAccount.physicalMember.phoneNumber}}
                                    {% endif %}
                                {% else %}
                                    {% if operation.depositAccount.moralMember %}
                                        {{operation.depositAccount.moralMember.phoneNumber}}
                                    {% else %}
                                        {{operation.depositAccount.physicalMember.phoneNumber}}
                                    {% endif %}
                                {% endif %}
                                    </b>
        </td>
        </tr>
        <tr>
            <td class="50p">Amount: <b>{{operation.amount}}</b></td>
            <td class="50p"></td>
        </tr>
    </table><br>
<br><br><br>

<table style="margin-bottom:0px">
    <tr>
        <td class="25p">XAF: <b>F CFA</b></td>
        <td class="25p" style="text-align:right">Account: <b>
            {% if operation.savingAccount %}
                {{operation.savingAccount}}

            {% elseif operation.shareAccount %}
                {{operation.shareAccount}}
            {% else %}
                {{operation.depositAccount}}
            {% endif %}
        </b></td>
        <td class="25p" style="text-align:right">
            <b> 
                                {% if operation.savingAccount %}
                                    {% if operation.savingAccount.moralMember %}
                                        {{operation.savingAccount.moralMember[:17]|upper}}
                                    {% else %}
                                        {{operation.savingAccount.physicalMember[:17]|upper}}
                                    {% endif %}

                                {% elseif operation.shareAccount %}
                                    {% if operation.shareAccount.moralMember %}
                                        {{operation.shareAccount.moralMember[:17]|upper}}
                                    {% else %}
                                        {{operation.shareAccount.physicalMember[:17]|upper}}
                                    {% endif %}
                                {% else %}
                                    {% if operation.depositAccount.moralMember %}
                                        {{operation.depositAccount.moralMember[:17]|upper}}
                                    {% else %}
                                        {{operation.depositAccount.physicalMember[:17]|upper}}
                                    {% endif %}
                                {% endif %}

                            </b>
        </td>
        <td class="25p" style="text-align:right">
            Amount: <b>{{operation.amount}}</b>
        </td>
    </tr>
</table><br>

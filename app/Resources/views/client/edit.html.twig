   <div class="row">
        {{ form_start(edit_form, {'method': 'POST', 'action': path('client_edit',{id:client.id} )}) }}
        <div class="col-md-offset-1 col-md-10 col-sm-12">
            <fieldset>
                <legend></legend>
                    <div class="form-group">
                        {{form_label(edit_form.name)}}
                        {{form_errors(edit_form.name)}}
                        {{form_widget(edit_form.name, {'attr':{'class':'form-control'}})}}
                    </div>
                    <div class="form-group">
                        {{form_label(edit_form.collector)}}
                        {{form_errors(edit_form.collector)}}
                        {{form_widget(edit_form.collector, {'attr':{'class':'form-control'}})}}
                    </div>
                    <div class="form-group">
                        <input class="btn btn-default" type="submit" value="Save" />    
                    </div>
            </fieldset>
        </div>
        {{ form_end(edit_form) }}
    </div>